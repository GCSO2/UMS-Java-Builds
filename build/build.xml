<?xml version="1.0" encoding="ISO-8859-1" ?>

<project default="Release_MobileServer" basedir=".">

	<property file="build.properties" />

	<property name="ums.util.workspace" value="${basedir}/../../${ums.util.workspace.path}" />	
	<property name="ums.stager.workspace" value="${basedir}/../../${ums.stager.workspace.path}" />		
	<property name="ums.fomobile.workspace" value="${basedir}/../../${ums.fomobile.workspace.path}" />
	<property name="ums.womobile.workspace" value="${basedir}/../../${ums.womobile.workspace.path}" />		
	<property name="ums.mobileserver.workspace" value="${basedir}/../../${ums.mobileserver.workspace.path}" />
	<property name="ums.mobileutil.workspace" value="${basedir}/../../${ums.mobileutil.workspace.path}" />
	<property name="ums.remoteupdatemobile.workspace" value="${basedir}/../../${ums.mobileutil.workspace.path}" />
	<property name="ums.remoteupdates.server.workspace" value="${basedir}/../../${ums.remotesupdates.serverapp.workspace.path}" />

	<path id="ums.util.lib.classpath">
		<fileset dir="${ums.util.workspace}/lib">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.util.build.classpath">
		<fileset dir="${ums.util.workspace}/build">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.mobileserver.lib.classpath">
		<fileset dir="${ums.mobileserver.workspace}/lib">
			<include name="**/*.jar" />
		</fileset>
	</path>


	<path id="ums.remotesupdates.lib.classpath">
		<fileset dir="${ums.remoteupdates.server.workspace}/lib">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.mobileserver.build.classpath">
		<fileset dir="${ums.mobileserver.workspace}/build">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.remotesupdates.serverapp.build.classpath">
		<fileset dir="${ums.remoteupdates.server.workspace}/build">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.stager.build.classpath">
		<fileset dir="${ums.stager.workspace}/build">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.remoteupdatemobile.build.classpath">
		<fileset dir="${ums.remoteupdatemobile.workspace}/build">
			<include name="**/*.jar" />
		</fileset>
	</path>

	<path id="ums.remoteupdatemobile.lib.classpath">
		<fileset dir="${ums.remoteupdatemobile.workspace}/lib">
			<include name="**/*.jar" />
		</fileset>
	</path>
	
	
	<path id="ums.mobileutil.build.classpath">
			<fileset dir="${ums.mobileutil.workspace}/build">
				<include name="**/*.jar" />
			</fileset>
	</path>

	<path id="ums.mobileutil.lib.classpath">
			<fileset dir="${ums.mobileutil.workspace}/lib">
				<include name="**/*.jar" />
			</fileset>
	 </path>


	<target name="create-jar-ums-util">
		<mkdir dir="${ums.util.workspace}/build/classes" />
		<delete file="${ums.util.workspace}/build/${ums.util.jarname}" />
		<javac debug="false" deprecation="true" destdir="${ums.util.workspace}/build/classes" srcdir="${ums.util.workspace}/src">
			<classpath refid="ums.util.lib.classpath" />
		</javac>
		<jar jarfile="${ums.util.workspace}/build/${ums.util.jarname}">
			<fileset dir="${ums.util.workspace}/build/classes">
				<include name="**/*.class" />
			</fileset>
			<fileset dir="${ums.util.workspace}/src">
				<include name="**/.control" />
			</fileset>
		</jar>
		<delete dir="${ums.util.workspace}/build/classes" />
	</target>



	<target name="create-jar-ums-server" depends="create-jar-ums-util">
		<mkdir dir="${ums.mobileserver.workspace}/build/classes_inter" />
		<mkdir dir="${ums.mobileserver.workspace}/build/classes_impl" />

		<delete file="${ums.mobileserver.workspace}/build/${ums.mobileserver.jarname.interface}" />
		<delete file="${ums.mobileserver.workspace}/build/${ums.mobileserver.jarname.impl}" />

		<javac debug="true" deprecation="true" destdir="${ums.mobileserver.workspace}/build/classes_inter" srcdir="${ums.mobileserver.workspace}/inter">
			<classpath refid="ums.util.lib.classpath" />
			<classpath refid="ums.util.build.classpath" />
		</javac>

		<jar jarfile="${ums.mobileserver.workspace}/build/${ums.mobileserver.jarname.interface}">
			<fileset dir="${ums.mobileserver.workspace}/build/classes_inter">
				<include name="**/*.class" />
			</fileset>
		</jar>

		<javac debug="true" deprecation="true" destdir="${ums.mobileserver.workspace}/build/classes_impl" srcdir="${ums.mobileserver.workspace}/impl">
			<classpath refid="ums.util.lib.classpath" />
			<classpath refid="ums.util.build.classpath" />
			<classpath refid="ums.mobileserver.lib.classpath" />
			<classpath path="${ums.mobileserver.workspace}/build/${ums.mobileserver.jarname.interface}" />
		</javac>


		<jar jarfile="${ums.mobileserver.workspace}/build/${ums.mobileserver.jarname.impl}">
			<fileset dir="${ums.mobileserver.workspace}/build/classes_impl">
				<include name="**/*.class" />
			</fileset>
			<fileset dir="${ums.mobileserver.workspace}/impl">
				<include name="**/.control" />
			</fileset>
		</jar>

		<delete dir="${ums.mobileserver.workspace}/build/classes_inter" />
		<delete dir="${ums.mobileserver.workspace}/build/classes_impl" />
	</target>




	<target name="create-jar-remoteupdates.server" depends="create-jar-ums-util">
		<mkdir dir="${ums.remoteupdates.server.workspace}/build/classes_inter" />
		<mkdir dir="${ums.remoteupdates.server.workspace}/build/classes_impl" />
		<mkdir dir="${ums.remoteupdates.server.workspace}/build/classes_shared" />

		<delete file="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.interface}" />
		<delete file="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.impl}" />
		<delete file="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.shared}" />

		<!-- Interfaces -->
		<javac debug="true" deprecation="true" destdir="${ums.remoteupdates.server.workspace}/build/classes_inter" srcdir="${ums.remoteupdates.server.workspace}/inter">
			<classpath refid="ums.util.lib.classpath" />
			<classpath refid="ums.util.build.classpath" />
		</javac>
		<jar jarfile="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.interface}">
			<fileset dir="${ums.remoteupdates.server.workspace}/build/classes_inter">
				<include name="**/*.class" />
			</fileset>
		</jar>

		<!-- Shared classes -->
		<javac debug="true" deprecation="true" destdir="${ums.remoteupdates.server.workspace}/build/classes_shared" srcdir="${ums.remoteupdates.server.workspace}/shared">
			<classpath refid="ums.util.lib.classpath" />
			<classpath refid="ums.util.build.classpath" />
			<classpath refid="ums.remotesupdates.lib.classpath" />
			<classpath path="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.interface}" />
			<classpath path="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.shared}" />
		</javac>
		<jar jarfile="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.shared}">
			<fileset dir="${ums.remoteupdates.server.workspace}/build/classes_shared">
				<include name="**/*.class" />
			</fileset>
			<fileset dir="${ums.remoteupdates.server.workspace}/shared">
				<include name="**/.control" />
			</fileset>
		</jar>


		<!-- Implementations -->
		<javac debug="true" deprecation="true" destdir="${ums.remoteupdates.server.workspace}/build/classes_impl" srcdir="${ums.remoteupdates.server.workspace}/impl">
			<classpath refid="ums.util.lib.classpath" />
			<classpath refid="ums.util.build.classpath" />
			<classpath refid="ums.remotesupdates.lib.classpath" />
			<classpath path="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.shared}" />
			<classpath path="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.interface}" />
		</javac>
		<jar jarfile="${ums.remoteupdates.server.workspace}/build/${ums.remotesupdates.serverapp.jarname.impl}">
			<fileset dir="${ums.remoteupdates.server.workspace}/build/classes_impl">
				<include name="**/*.class" />
			</fileset>
			<fileset dir="${ums.remoteupdates.server.workspace}/impl">
				<include name="**/.control" />
			</fileset>
		</jar>

		<delete dir="${ums.remoteupdates.server.workspace}/build/classes_inter" />
		<delete dir="${ums.remoteupdates.server.workspace}/build/classes_impl" />
		<delete dir="${ums.remoteupdates.server.workspace}/build/classes_shared" />
	</target>



	<target name="Release_MobileServer" depends="create-jar-ums-server">
		<delete file="${ums.mobileserver.release}/${ums.mobileserver.version}/lib/${ums.mobileserver.jarname.interface}" />
		<delete file="${ums.mobileserver.release}/${ums.mobileserver.version}/lib/${ums.mobileserver.jarname.impl}" />
		<delete file="${ums.mobileserver.release}/${ums.mobileserver.version}/lib/${ums.util.jarname}" />

		<mkdir dir="${ums.mobileserver.release}/${ums.mobileserver.version}/lib" />
		<copy todir="${ums.mobileserver.release}/${ums.mobileserver.version}/lib">
			<fileset dir="${ums.mobileserver.workspace}/build" defaultexcludes="true">
				<include name="${ums.mobileserver.jarname.interface}" />
				<include name="${ums.mobileserver.jarname.impl}" />
			</fileset>
			<fileset dir="${ums.util.workspace}/build" defaultexcludes="true">
				<include name="${ums.util.jarname}" />
			</fileset>
		</copy>
	</target>




	<target name="Release_RemoteUpdatesServer" depends="create-jar-remoteupdates.server">
		<delete file="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib/${ums.remotesupdates.serverapp.jarname.interface}" />
		<delete file="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib/${ums.remotesupdates.serverapp.jarname.impl}" />
		<delete file="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib/${ums.remotesupdates.serverapp.jarname.shared}" />
		<delete file="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib/${ums.util.jarname}" />

		<mkdir dir="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib" />
		<copy todir="${ums.remotesupdates.serverapp.release}/${ums.remotesupdates.serverapp.version}/lib">
			<fileset dir="${ums.remoteupdates.server.workspace}/build" defaultexcludes="true">

			</fileset>
			<fileset dir="${ums.util.workspace}/build" defaultexcludes="true">
				<include name="${ums.util.jarname}" />
			</fileset>
		</copy>
	</target>


	
